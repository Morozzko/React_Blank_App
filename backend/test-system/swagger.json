  "info": {
    "title": "Version Synchronization",
    "description": "Microservice for Version Synchronization",
    "version": "0.0.3",
    "contact": {}
  },
  "tags": [],
  "servers": [
    {
      "url": "http://localhost:3000",
      "description": "standalone"
    }
  ],
  "components": {
    "schemas": {
      "SyncLibraryRequestDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "Уникальное имя библиотеки для обновления",
            "example": "SomeLibrary"
          },
          "version": {
            "type": "string",
            "description": "Новая версия библиотеки",
            "example": "1.0.1"
          }
        },
        "required": [
          "name",
          "version"
        ]
      },
      "SyncLibraryResponseDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "Имя библиотеки",
            "example": "SomeLibrary"
          },
          "version": {
            "type": "string",
            "description": "Подтвержденная версия библиотеки",
            "example": "1.0.1"
          },
          "isBanned": {
            "type": "boolean",
            "description": "Запрещена ли библиотека",
            "example": true
          },
          "libraryForMigrate": {
            "type": "string",
            "description": "Библиотека на которую нужно перейти",
            "example": "AnotherLibrary"
          }
        },
        "required": [
          "name",
          "version",
          "isBanned",
          "libraryForMigrate"
        ]
      },
      "CreateReviewDto": {
        "type": "object",
        "properties": {}
      },
      "UpdateReviewDto": {
        "type": "object",
        "properties": {}
      }
    }
  }
}